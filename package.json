{
  "name": "evercity-frontend-service",
  "private": true,
  "version": "0.0.1",
  "description": "evercity service",
  "author": {
    "name": "Evercity Team"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --ext .tsx,.ts src/",
    "format": "prettier --write src/",
    "api:dev": "DOTENV_CONFIG_PATH=./.env.development graphql-codegen -r dotenv/config",
    "api:prod": "DOTENV_CONFIG_PATH=./.env.production graphql-codegen -r dotenv/config",
    "prepare": "husky install",
    "dep-graph": "depcruise src --include-only \"^src\" --config --output-type dot | dot -T svg > dependency-graph.svg",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome --headless"
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@evercityecosystem/evercity-ui": "1.2.6",
    "graphql": "^16.6.0",
    "i18next": "^22.4.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.5",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@graphql-codegen/add": "^4.0.0",
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/lodash.difference": "^4.5.7",
    "@types/lodash.range": "^3.2.7",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "cypress": "^12.8.1",
    "dependency-cruiser": "^12.10.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-boundaries": "^3.1.0",
    "eslint-plugin-fsd-import": "^0.0.12",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-tsconfig-paths": "^4.0.5"
  }
}
